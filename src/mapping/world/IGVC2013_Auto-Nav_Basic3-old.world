include "stage_igvc_obstacles.inc"
include "stage_igvc_waypoints.inc"
include "husky_camera.inc"
include "cerberus_camera.inc"

define myblock model
(
  size [0.5 0.5 0.5]
  gui_nose 0
)


#husky_base
#(		  
#  # can refer to the robot by this name
#  name "Robot"
#  pose [ -1.408 -7.517 0.000 149.170 ] 
#  localization "gps"
#  localization_origin [ 0 0 0 0 ]
#)
 
cerberus
(		  
  # can refer to the robot by this name
  name "Cerberus"
  pose [  -32.747 18.562 0.000 -97.436 ] 
  localization "gps"
  localization_origin [ 0 0 0 0 ]
)
 
 
 
define floorplan model
(
  # sombre, sensible, artistic
  color "gray30"

  # most maps will need a bounding box
  boundary 1

  gui_nose 0
  gui_grid 0

  gui_outline 0
  gripper_return 0
  fiducial_return 0
  ranger_return 1.000
)

define floorlaneline model
(
  # sombre, sensible, artistic
  color "white"

  # most maps will need a bounding box
  boundary 1

  gui_nose 0
  gui_grid 0

  gui_outline 0
  gripper_return 0
  fiducial_return 1
  
)



# set the resolution of the underlying raytrace model in meters
resolution 0.02

interval_sim 100  # simulation timestep in milliseconds


window
( 
#The window size must larger then your camera image size 
  size [ 800 693 ] 

  rotate [ 0.000 0.000 ]
  scale 21.436 
)
floorplan
(
  name "grass"
  size [150.000 90.000 0.001 ]
  pose [ 0.000 0.000 0.000 90.000]
  color "ForestGreen"
  obstacle_return 0
  gui_move 0
)

# load an environment bitmap
#floorplan
#( 
#  name "empty"
#  bitmap "bitmaps/IGVC2013_Auto-Nav_Basic3.png"
#  size [32.000 32.000 0.800]
#  pose [0 0 0 0]
#  gui_move 0
#)
# Draw lanes
floorplan
( 
  name "igvc"
  size [ 131.600 81.700 0.002 ]
  pose [ 0.100 0.100 0.000 -90.000]
  
  # IGVC2013_Auto-Nav_Basic.png image size 1316x817 = 131.6x81.7 meter
  bitmap "bitmaps/IGVC2013_Auto-Nav_Basic.png"
  gui_outline 0
  obstacle_return 1
  ranger_return 1.000
  color "white"
  gui_move 0
)



